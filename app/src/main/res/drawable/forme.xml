<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="80dp" android:height="10dp">
        <rotate android:fromDegrees="-15" android:pivotX="0%" android:pivotY="50%">
            <shape android:shape="rectangle">
                <solid android:color="#50C878"/>
                <corners android:radius="30dp"/>
            </shape>
        </rotate>
    </item>
    <item android:width="80dp" android:height="10dp" android:top="20dp">
        <rotate android:fromDegrees="-15" android:pivotX="0%" android:pivotY="50%">
            <shape android:shape="rectangle">
                <solid android:color="#50C878"/>
                <corners android:radius="30dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>